{
    "name": "difra/cms",
    "description": "CMS plugin for Difra framework",
    "type": "library",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Eduard Kutuev",
            "email": "nap@napych.com"
        }
    ],
    "dist": {
        "url": "https://github.com/difra-org/cms/archive/1.0.2.zip",
        "type": "zip"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:difra-org/cms.git"
        }
    ],
    "require": {
        "difra/difra": "~7.0.0",
        "difra/editor": "~1.0.0"
    },
    "autoload": {
        "files": [
            "main.php"
        ],
        "classmap": [
            "src/CMS.php"
        ],
        "psr-4": {
            "Difra\\CMS\\": "src"
        }
    }
}
